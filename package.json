    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest": "^22.4.3",
    "jest-enzyme": "^6.0.0",
    "randomstring": "^1.1.5",
    "react-test-renderer": "^16.3.2",
    "webpack-cli": "^2.1.3"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "busboy-body-parser": "^0.3.2",
    "concurrently": "^3.5.1",
    "cors": "^2.8.4",
    "email-verification": "^0.4.6",
